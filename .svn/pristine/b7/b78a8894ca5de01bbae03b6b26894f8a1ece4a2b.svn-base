# must be unique in a given SonarQube instance
sonar.projectKey=org:marmots:generator
# this is the name displayed in the SonarQube UI
sonar.projectName=marmots-generator
sonar.projectVersion=1.0
 
# locations
sonar.sources=src/main/java
sonar.java.binaries=target/classes

# test and coverage
sonar.tests=src/test/java
sonar.java.test.binaries=target/test-classes
sonar.junit.reportPaths=target/surefire-reports
sonar.jacoco.reportPaths=target/jacoco.exec, target/jacoco-it.exec

# exclusions (by the moment)
sonar.exclusions=**/*.ts, **/*.js
 
# Encoding of the source code. Default is default system encoding
sonar.sourceEncoding=UTF-8

# java version used by source files:
sonar.java.source=1.8